<h1><%= @kitten.name %></h1>

<p>Age: <%= @kitten.age %></p>
<p>Softness: <%= @kitten.softness %></p>
<p>Cuteness: <%= @kitten.cuteness %></p>

<%= link_to "Home", kittens_path %>
<%= link_to "Edit", edit_kitten_path(@kitten) %>
<%= link_to "Destroy", kitten_path(@kitten), data: {
                    turbo_method: :delete,
                    turbo_confirm: "Are you sure?"
                  } %>